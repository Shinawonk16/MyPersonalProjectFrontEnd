                        a.page-link(href='#') 2
                      li.page-item
                        a.page-link(href='#') 3
                      li.page-item
                        a.page-link(href='#')
                          i.fa.fa-angle-right
            .col-xxl-4.col-md-6            
              .card.buttons
                .card-header
                  h4 info pagination
                .card-body
                  nav(aria-label='...')
