            .col-xxl-4            
              .card.buttons
                .card-header
                  h4 danger pagination
                .card-body
                  nav(aria-label='...')
                    ul.pagination.pagination-danger
                      li.page-item
                        a.page-link(href='#' tabindex='-1') 
                          i.fa.fa-angle-left
                      li.page-item
                        a.page-link(href='#') 1
                      li.page-item.active(aria-current='page')
                        a.page-link(href='#') 2
                      li.page-item
                        a.page-link(href='#') 3
                      li.page-item
                        a.page-link(href='#')
                          i.fa.fa-angle-right
            .col-12            
              .card.buttons
                .card-header
                  h4 sizing pagination
                .card-body.overflow-hidden
                  .row
                    .col-xxl-4.my-3
                      nav(aria-label='...')
                        ul.pagination.pagination-lg
                          li.page-item
                            a.page-link(href='#') Previous
                          li.page-item
                            a.page-link(href='#') 1
                          li.page-item
                            a.page-link(href='#') 2
                          li.page-item
                            a.page-link(href='#') 3
                          li.page-item
                            a.page-link(href='#') Next
                    .col-xxl-4.my-3
                      nav(aria-label='...')
                        ul.pagination.pagination-md
                          li.page-item
                            a.page-link(href='#') Previous
                          li.page-item
